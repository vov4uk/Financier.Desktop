<UserControl x:Class="Financier.Reports.Reports.Filters.StartYearMonthFilter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fcrd="clr-namespace:Financier.Reports.Common"
             Width="120">
  <GroupBox Header="From">
    <ComboBox Grid.Column="1" DisplayMemberPath="Name"
              SelectedItem="{Binding Path=StartYearMonths}"
              ItemsSource="{Binding Source={x:Static fcrd:DbManual.YearMonths}, Mode=OneWay}" />
  </GroupBox>
</UserControl>